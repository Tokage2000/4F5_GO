<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import GO.pages.partie.composants.ConteneurGrille?>

<VBox xmlns:fx="http://javafx.com/fxml"
	  styleClass="conteneurPrincipal" 
      fx:controller="GO.pages.partie.vues.VuePartieReseau"
	  VBox.vgrow="ALWAYS">
       <VBox styleClass="conteneurTableJeu" VBox.vgrow="ALWAYS">
               <ConteneurGrille fx:id="conteneurGrille" styleClass="conteneurGrille" couleurNoir="#000000" couleurBlanc="#FFFFFF" VBox.vgrow="ALWAYS"/>
    </VBox>
	<VBox styleClass="petitEspaceVertical"/>
    <HBox styleClass="conteneurNomsJoueurs" VBox.vgrow="SOMETIMES">
            <Text fx:id="nomJoueurUn" styleClass="conteneurNomJoueur" text="%partieReseau"/>
    </HBox>
</VBox>
