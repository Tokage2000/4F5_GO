<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>
<?import GO.pages.replay.composants.ConteneurGrilleRP?>

<VBox xmlns:fx="http://javafx.com/fxml" 
	styleClass="conteneurPrincipal"
      fx:controller="GO.pages.replay.vues.VueReplay"
      VBox.vgrow="ALWAYS">

       <VBox styleClass="conteneurTableJeu" VBox.vgrow="ALWAYS">
               <ConteneurGrilleRP fx:id="conteneurGrilleRP" styleClass="conteneurGrille" couleurNoir="#000000" couleurBlanc="#FFFFFF" VBox.vgrow="ALWAYS"/>
    </VBox>
    <VBox>
    <HBox alignment="center">
        <Button text="Précédant" fx:id="precedant" />
        <Button text="Suivant" fx:id="suivant" />
    </HBox>
    </VBox>
    <VBox>
        <HBox alignment="center">
        <Button text="Quitter" fx:id="quitter" />
    </HBox>
    </VBox>
</VBox>